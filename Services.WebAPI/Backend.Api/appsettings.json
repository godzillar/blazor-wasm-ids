{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Database": {
    "Name": "ApiDatabase",
    "Type": "SQL",
    "ConnectionString": "Data Source=.;Initial Catalog=ApiDatabase;Integrated Security=True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Debug",
    "WriteTo": [ {"Name":  "Console"} ]
  }
}
